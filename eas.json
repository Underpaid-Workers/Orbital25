{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env":{
        "HF_TOKEN":"HF_TOKEN",
        "HF_MODEL_URL" : "HF_MODEL_URL",
        "GOOGLE_MAPS_API_KEY":"GOOGLE_MAPS_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env":{
        "HF_TOKEN":"HF_TOKEN",
        "HF_MODEL_URL" : "HF_MODEL_URL",
        "GOOGLE_MAPS_API_KEY":"GOOGLE_MAPS_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
